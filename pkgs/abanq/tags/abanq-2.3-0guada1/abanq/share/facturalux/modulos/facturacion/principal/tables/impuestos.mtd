<!DOCTYPE TMD>
<TMD>
	<name>impuestos</name>
	<!--Listado de impuestos-->
	<alias>QT_TRANSLATE_NOOP("MetaData","Impuestos")</alias>

	<field>
		<name>codimpuesto</name>
		<!--Código del impuesto-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Código")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>string</type>
		<length>10</length>

		<relation>
			<table>impuestos_imp</table>
			<field>desde</field>
			<card>1M</card>
		</relation>

		<relation>
			<table>impuestos_imp</table>
			<field>hasta</field>
			<card>1M</card>
		</relation>

