		</relation>

		<relation>
			<table>lineasivafactprov</table>
			<field>codimpuesto</field>
			<card>1M</card>
		</relation>

		<relation>
			<table>lineasivafactcli</table>
			<field>codimpuesto</field>
			<card>1M</card>
		</relation>

		<relation>
			<table>co_subcuentas</table>
			<field>codimpuesto</field>
			<card>1M</card>
		</relation>

	</field>

	<field>
		<name>descripcion</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Descripción")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>50</length>
	</field>

	<field>
		<name>iva</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","% I.V.A.")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>double</type>
		<partI>2</partI>
		<partD>2</partD>
	</field>

	<field>
		<name>recargo</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","% Rec. equivalencia")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>double</type>
		<partI>2</partI>
		<partD>2</partD>
	</field>
	
	<field>
		<name>codsubcuentarep</name>
		<!-- Código de la cuenta de IVA repercutido de contabilidad a la que se asociarán las partidas de iva de asociadas a este código de impuesto-->
		<alias>QT_TRANSLATE_NOOP("MetaData","I.V.A. Repercutido")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>15</length>
	</field>

	<field>
		<name>idsubcuentarep</name>
