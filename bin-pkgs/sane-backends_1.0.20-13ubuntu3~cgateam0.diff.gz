++      retry:
++	e2_recv(s, result, s->block ? 6 : 4, &status);
++	if (status != SANE_STATUS_GOOD)
++		return status;
++
++	if (result->code != STX) {
++		DBG(1, "error: got %02x, expected STX\n", result->code);
++		return SANE_STATUS_INVAL;
