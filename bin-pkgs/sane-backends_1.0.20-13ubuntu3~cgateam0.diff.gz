++	    && mparam->flags == 0x02) {
++		buf[24] = 0x13;
++	} else {
++		buf[24] = mparam->flags | (mparam->dropout_mask
++					   & dropout_params[s->
++							    val[OPT_DROPOUT].
++							    w]);
++	}
++
++	/* ESC D, set data format */
++	mparam = &mode_params[s->val[OPT_MODE].w];
++	buf[25] = mparam->depth;
++
++	/* ESC e, control option */
++	if (s->hw->extension) {
