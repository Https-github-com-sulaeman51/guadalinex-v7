++static int
++get_roundup_index(double frac[], int n)
++{
++	int i, index = -1;
++	double max_val = 0.0;
++
++	for (i = 0; i < n; i++) {
++
++		if (frac[i] < 0)
++			continue;
++
++		if (max_val < frac[i]) {
++			index = i;
++			max_val = frac[i];
++		}
++	}
++
++	return index;
++}
++
++static int
++get_rounddown_index(double frac[], int n)
++{
++	int i, index = -1;
++	double min_val = 1.0;
++
++	for (i = 0; i < n; i++) {
++
++		if (frac[i] > 0)
++			continue;
++
++		if (min_val > frac[i]) {
++			index = i;
++			min_val = frac[i];
++		}
++	}
++
++	return index;
++}
