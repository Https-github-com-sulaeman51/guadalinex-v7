++static inline int
++get_color(int status)
++{
++	switch ((status >> 2) & 0x03) {
++	case 1:
++		return 1;
++	case 2:
++		return 0;
++	case 3:
++		return 2;
++	default:
++		return 0;	/* required to make the compiler happy */
++	}
++}
