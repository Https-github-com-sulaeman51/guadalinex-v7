+-static SANE_Status
+-e2_start_std_scan(Epson_Scanner * s)
+-{
+-	SANE_Status status;
+-	unsigned char params[2];
+-
+-	DBG(5, "%s\n", __func__);
+-
+-	/* ESC g */
+-	params[0] = ESC;
+-	params[1] = s->hw->cmd->start_scanning;
