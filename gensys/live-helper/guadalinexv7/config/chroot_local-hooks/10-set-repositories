#!/bin/bash

cat > /etc/apt/sources.list << EOF
#TITLE: Sitio principal de Guadalinex en la Junta de Andalucia
#ID:jda

deb http://v7.guadalinex.org/ubuntu lucid main restricted universe multiverse
deb http://v7.guadalinex.org/guadalinex-lince lince main restricted universe multiverse
deb http://v7.guadalinex.org/guadalinex-lince lince-updates main restricted universe multiverse
deb http://v7.guadalinex.org/guadalinex-lince lince-security main restricted universe multiverse
deb http://v7.guadalinex.org/guadalinex-lince lince-backports main restricted universe multiverse
#END
EOF
