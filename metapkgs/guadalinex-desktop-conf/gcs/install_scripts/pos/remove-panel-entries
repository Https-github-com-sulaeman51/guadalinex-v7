if [ -f /usr/share/gconf/defaults/05_panel-default-setup.entries ]
then
	mv /usr/share/gconf/defaults/05_panel-default-setup.entries /usr/share/guadalinex-desktop-conf/05_panel-default-setup.entries.distrib
fi

cp /usr/share/guadalinex-desktop-conf/05_panel-default-setup.entries /usr/share/gconf/defaults/05_panel-default-setup.entries
dpkg-divert --package  guadalinex-desktop-conf --quiet --add /usr/share/gconf/defaults/05_panel-default-setup.entries --divert /usr/share/guadalinex-desktop-conf/05_panel-default-setup.entries.distrib
